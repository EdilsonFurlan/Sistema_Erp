[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/Sistema_Erp
ExecStart=/home/ubuntu/Sistema_Erp/venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/Sistema_Erp/sistema.sock core.wsgi:application

[Install]
WantedBy=multi-user.target
